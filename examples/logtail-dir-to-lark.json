{
  "port": 54321,
  "transfers": {
    "lark-alarm": {
      "name": "lark-alarm",
      "type": "lark",
      "prefix": "Log Alarm",
      "url": "https://open.feishu.cn/open-apis/bot/v2/hook/6b4d507c-b6ee-46d7-8150-xxxxxxxxx"
    }
  },
  "routers": {
    "xxx-router": {
      "name": "xxx-router",
      "matchers": [
        {
          "contains": [
            "ERROR"
          ],
          "not_contains": [
            "Invalid",
            "NotFound"
          ]
        }
      ],
      "transfers": [
        "lark-alarm"
      ]
    }
  },
  "servers": {
    "xxx": {
      "name": "xxx",
      "routers": [
        "xxx-router"
      ],
      "file": {
        "path": "/logs/xxx-service/",
        "recursive": true,
        "suffix": ".log",
        "method": "timer"
      }
    }
  }
}
